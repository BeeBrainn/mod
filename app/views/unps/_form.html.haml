= form_for @unp do |f|
  -if @unp.errors.any?
    #error_explanation
      %h2= "#{pluralize(@unp.errors.count, "error")} prohibited this unp from being saved:"
      %ul
        - @unp.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :unp
    = f.text_field :unp
  .field
    = f.label :total_amount
    = f.text_field :total_amount
  .actions
    = f.submit 'Save'
